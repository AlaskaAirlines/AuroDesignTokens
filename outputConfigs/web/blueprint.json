{
  "source": ["./processed/web/Blueprint_theme.Light.json"],
  "include": [],
  "platforms": {
    "SCSSVariables": {
      "transformGroup": "scss",
      "prefix": "ds",
      "comment": "File type: SCSS; variable type: Sass",
      "buildPath": "./dist/web/",
      "files": [
        {
          "destination": "blueprint.scss",
          "format": "scss/variables"
        }
      ]
    },
    "CSSCustomProperties": {
      "transformGroup": "css",
      "prefix": "ds",
      "comment": "File type: CSS; variable type: CSS",
      "buildPath": "./dist/web/",
      "files": [
        {
          "destination": "blueprint.css",
          "format": "css/variables"
        }
      ]
    },
    "JSONVariables": {
      "transformGroup": "css",
      "prefix": "ds",
      "comment": "File type: JSON; variable type: CSS",
      "buildPath": "./dist/web/",
      "files": [
        {
          "destination": "JSONVariablesNested--test.json",
          "format": "json/nested"
        }
      ]
    },
    "JSObject--allTokens": {
      "transformGroup": "js",
      "prefix": "ds",
      "comment": "File type: JS; data type: JS",
      "buildPath": "./dist/web/",
      "transforms": [
        "attribute/cti",
        "name/cti/kebab",
        "color/css"
      ],
      "files": [
        {
          "destination": "JSObject--allTokens--test.js",
          "format": "javascript/module"
        }
      ]
    }
  }
}
