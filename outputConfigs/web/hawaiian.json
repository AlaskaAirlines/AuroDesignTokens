{
  "source": ["./processed/web/Hawaiian_theme.Light.json"],
  "include": [],
  "platforms": {
    "SCSSVariables": {
      "transformGroup": "scss",
      "prefix": "ds",
      "comment": "File type: SCSS; variable type: Sass",
      "buildPath": "./dist/web/",
      "files": [
        {
          "destination": "hawaiian.scss",
          "format": "scss/variables",
          "options": {
            "showFileHeader": false
          }
        }
      ]
    },
    "CSSCustomProperties": {
      "transformGroup": "css",
      "prefix": "ds",
      "comment": "File type: CSS; variable type: CSS",
      "buildPath": "./dist/web/",
      "files": [
        {
          "destination": "hawaiian.css",
          "format": "css/variables",
          "options": {
            "showFileHeader": false
          }
        }
      ]
    },
    "JSONVariables": {
      "transformGroup": "css",
      "prefix": "ds",
      "comment": "File type: JSON; variable type: CSS",
      "buildPath": "./dist/web/",
      "files": [
        {
          "destination": "JSONVariablesNested--hawaiian.json",
          "format": "json/nested",
          "options": {
            "showFileHeader": false
          }
        }
      ]
    },
    "JSObject--allTokens": {
      "transformGroup": "js",
      "prefix": "ds",
      "comment": "File type: JS; data type: JS",
      "buildPath": "./dist/web/",
      "transforms": [
        "attribute/cti",
        "name/cti/kebab",
        "color/css"
      ],
      "files": [
        {
          "destination": "JSObject--allTokens--hawaiian.js",
          "format": "javascript/module",
          "options": {
            "showFileHeader": false
          }
        }
      ]
    }
  }
}
