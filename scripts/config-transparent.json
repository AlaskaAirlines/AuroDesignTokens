{
  "source": ["./src/themes/transparent/color/*.json"],
  "include": ["./src/themes/transparent/*.json"],
  "platforms": {
    "CSSCustomProperties": {
      "transformGroup": "css",
      "prefix": "ds",
      "comment": "File type: CSS; variable type: CSS; filter: public",
      "buildPath": "./dist/transparent/",
      "files": [
        {
          "destination": "CSSCustomProperties--transparent.css",
          "format": "css/variables",
          "filter": {
            "public": true
          }
        }
      ]
    },
    "compose": {
      "transformGroup": "custom/compose-colors",
      "buildPath": "src/android/auroDesignTokens-sdk-android/src/main/kotlin/com/alaskaair/aurodesigntokens_sdk_android/",
      "files": [
        {
          "destination": "TransparentColors.kt",
          "format": "custom/formats/compose",
          "filter": {
            "attributes": {
              "category": "color"
            }
          },
          "options": {
            "className": "TransparentColors",
            "packageName": "com.alaskaair.aurodesigntokens_sdk_android",
            "objectType": "class"
          }
        }
      ]
    }
  }
}
